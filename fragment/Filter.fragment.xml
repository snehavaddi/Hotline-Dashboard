<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleFilterDialogConfirm">
		<filterItems>
			<ViewSettingsFilterItem
				text="Product Version"
				key="Product"
				multiSelect="true"
				items="{prods>/}">
				<items>
					<ViewSettingsItem text="{prods>GroupName}" key="{prods>GroupName}" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>